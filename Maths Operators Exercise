MyFloatValue = 9.82
print(type(MyFloatValue))
MyStringValue = str(MyFloatValue)
print(type(MyStringValue))
MyIntegerValue = int(MyFloatValue)
print(type(MyIntegerValue))
print("\n")

textValue = 'hello_world'
print(type(textValue))
numberText = int(textValue)
print(type(numberText))
 
