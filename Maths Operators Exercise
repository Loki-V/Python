mars_bar=float(1)
coke_can=float(1.50)
crisps=float(.80)
teacup=float(2)
slice_pan=float(3.50)

mars_bar=float(mars_bar*5)
coke_can=float(coke_can*4)
crisps=float(crisps*3)
teacup=float(teacup*2)

bill=mars_bar+coke_can+crisps+teacup+slice_pan

print(bill)


number1=int(input("Enter first number:"))
number2=int(input("Enter second number:"))
sum=number1+number2
print(f"{number1}+{number2}={sum}")


