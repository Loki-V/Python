#5

def fall():
    for i in range (0,3):
        x = int(input("Guess a number:"))
        if x >= 1 and x < 6:
            print('True')
        else:
            print('False')


print(fall())

#6
def lttrcnt(list):
    alpha = 0
    num = 0
    for i in list:
        if i.isupper() is True:
            alpha += 1
        elif i.islower() is True:
            num += 1
    response = (f"No. of Upper case characters:{alpha}\nNo. of Lower case characters:{num}")
    return response


input = input("Give me a sentece.\n")
print(lttrcnt(input))

#7
def function7(_):
    ans = []
    for i in range(0,len(_)):
        if _[i] in ans:
            continue  
        else: 
            ans.append(_[i])  
    return ans

samplelist = [1,2,3,3,3,3,4,5]
print(function8(samplelist))

#8

def function8(num):
    if num > 1:
        for i in range(2,num):
            if num % i == 0:
                response = (f"{num} is not a prime number.")
                break
            else:
                response = (f"{num} is a prime number.")
                break
    else:
        response = (f"{num} is not a prime number.")
    return response

number = int(input("Enter a number:"))
print(function8(number))


#9

def function9(num):
    ans = []
    for i in range(0,len(num)):
        if (i % 2) != 0:
            ans.append(num[i])
    return ans

SampleList= [1, 2, 3, 4, 5, 6, 7, 8, 9]
print(function9(SampleList))

